{
  "name": "react-boiler-plate",
  "version": "1.0.0",
  "description": "dual list box component for react",
  "main": "devServer.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:webpack:qa": "cross-env NODE_ENV=qa webpack --config webpack.config.js",
    "build:webpack:dev": "cross-env NODE_ENV=dev webpack --config webpack.config.js",
    "build": "npm run clean && npm version patch && npm run build:webpack:dev",
    "start": "node devServer.js",
    "test": "jest",
    "lint": "eslint --ignore-path ./.eslintignore ./src",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": "airbnb"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "radium": "^0.17.1",
    "react": "^0.14.7",
    "react-bootstrap": "^0.30.2",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^17.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.7",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-stats": "^0.1.4",
    "eventsource-polyfill": "^0.9.6",
    "flow-bin": "^0.26.0",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^17.0.1",
    "nock": "^9.0.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dashboard": "0.0.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.8.4"
  }
}
